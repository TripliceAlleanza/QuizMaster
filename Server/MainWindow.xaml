<Window x:Name="MainFormServer" x:Class="QuizMaster___Server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizMaster___Server"
        mc:Ignorable="d"
        Title="QuizMaster" Height="500" Width="1025" Background="White" WindowStartupLocation="CenterScreen">

   

        <!-- 
        <StackPanel Width="300" Height="auto" ></StackPanel>
        <StackPanel Width="300" Height="auto" ></StackPanel>
        <StackPanel Width="300" Height="auto" HorizontalAlignment="Right" />

        -->

        <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <DockPanel Grid.Column="0" Background="#FF0C0B0B" ></DockPanel>
        <DockPanel Grid.Column="1" Background="#FF161515" ></DockPanel>
        <DockPanel Grid.Column="2" Background="#FF0C0B0B" ></DockPanel>

    </Grid>
            
   
</Window>
